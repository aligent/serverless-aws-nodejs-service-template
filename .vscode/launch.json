    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug with Jest Test",
            "type": "node",
            "request": "attach",
            "address": "localhost",
            "port": 9229,
            "sourceMaps": true,
            "localRoot": "${workspaceFolder}",
            "remoteRoot": "/app",
            "outFiles": [
                "${workspaceFolder}/.esbuild/**/*.js"
            ],
            "internalConsoleOptions": "neverOpen",
            "preLaunchTask": "sls-docker: debug-test"
        },
        {
            "name": "Debug Invoke Local Json Path",
            "type": "node",
            "request": "attach",
            "address": "localhost",
            "port": 9229,
            "sourceMaps": true,
            "localRoot": "${workspaceFolder}",
            "remoteRoot": "/app",
            "outFiles": [
                "${workspaceFolder}/.esbuild/**/*.js"
            ],
            "internalConsoleOptions": "neverOpen",
            "preLaunchTask": "sls-docker: debug-invoke-local-path"
        },
        {
            "name": "Debug Invoke Local String Data",
            "type": "node",
            "request": "attach",
            "address": "localhost",
            "port": 9229,
            "sourceMaps": true,
            "localRoot": "${workspaceFolder}",
            "remoteRoot": "/app",
            "outFiles": [
                "${workspaceFolder}/.esbuild/**/*.js"
            ],
            "internalConsoleOptions": "neverOpen",
            "preLaunchTask": "sls-docker: debug-invoke-local-data"
        }
    ]
}