    "@nx/eslint-plugin": "20.0.6",
    "@nx/js": "20.0.6",
    "@nx/plugin": "20.0.6",
    "@nx/storybook": "20.0.6",
    "@nx/vite": "20.0.6",
    "@nx/web": "20.0.6",
    "@nx/workspace": "20.0.6",
