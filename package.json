{
  "name": "@aligent/serverless-aws-nodejs-service-template",
  "version": "3.1.0",
  "description": "Aligent AWS microservices template using Typescript and Serverless Framework",
  "scripts": {
    "test": "nx affected -t test --coverage",
    "test:all": "nx run-many -t test --coverage",
    "lint": "nx affected -t lint",
    "lint:all": "nx run-many -t lint",
    "format": "prettier . --check",
    "format:write": "prettier . --write",
    "check-types": "nx affected -t check-types",
    "check-types:all": "nx run-many -t check-types",
    "prepare": "[ -d .git ] && git config core.hooksPath '.git-hooks' || true"
  },
  "dependencies": {
    "@aws-sdk/client-sns": "^3.734.0"
  },
  "devDependencies": {
    "@aligent/serverless-conventions": "^1.0.0",
    "@aligent/ts-code-standards": "^2.0.2",
    "@nx/devkit": "20.4.0",
    "@nx/esbuild": "20.4.0",
    "@nx/eslint": "20.4.0",
    "@nx/eslint-plugin": "20.4.0",
    "@nx/js": "20.4.0",
    "@nx/plugin": "20.4.0",
    "@nx/vite": "20.4.0",
    "@nx/web": "20.4.0",
    "@nx/workspace": "20.4.0",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/aws-lambda": "^8.10.147",
    "@types/node": "20.12.12",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "esbuild": "~0.19.2",
    "eslint": "^9.19.0",
    "eslint-plugin-import": "^2.31.0",
    "nx": "20.4.0",
    "prettier": "^3.4.2",
    "serverless": "^3.38.0",
    "serverless-esbuild": "^1.54.6",
    "serverless-step-functions": "^3.21.2",
    "tsx": "^4.19.2",
    "typescript": "~5.7.3",
    "vite": "^5.4.14",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": "^20.18.0",
    "npm": "^10.8.2"
  },
