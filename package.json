    "@nx/eslint-plugin": "19.8.3",
    "@nx/js": "19.8.3",
    "@nx/plugin": "19.8.3",
    "@nx/storybook": "19.8.3",
    "@nx/vite": "19.8.3",
    "@nx/web": "19.8.3",
    "@nx/workspace": "19.8.3",
    "@redocly/cli": "^1.25.7",
    "@redocly/openapi-core": "^1.25.5",
    "@storybook/addon-interactions": "^8.2.8",
