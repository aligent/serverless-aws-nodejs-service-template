{
  "name": "base",
  "version": "1.0.0",
  "description": "Serverless ESBuild example using Typescript",
  "main": "handler.js",
  "type": "module",
  "scripts": {
    "jest-test-debug": "DOTENV_CONFIG_PATH=./development/.dev.env && ./node_modules/.bin/jest --runInBand",
    "invoke-local-debug": "DOTENV_CONFIG_PATH=./development/.dev.env && ./node_modules/.bin/serverless invoke local",
    "serverless-local": "DOTENV_CONFIG_PATH=./development/.dev.env && ./node_modules/.bin/serverless",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest --silent --coverage",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.216.0",
    "@aws-sdk/client-s3": "^3.216.0",
    "@aws-sdk/client-ssm": "^3.216.0",
    "axios": "^1.2.0"
  },
  "devDependencies": {
    "@aligent/serverless-conventions": "latest",
    "@types/aws-lambda": "^8.10.108",
    "@types/jest": "^29.2.3",
    "@types/node": "~18.11.9",
    "@types/serverless": "^3.12.9",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "asl-validator": "^3.0.5",
    "dotenv": "^16.0.3",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^29.3.1",
    "prettier": "2.8.0",
    "serverless-esbuild": "^1.33.2",
    "serverless-localstack": "^1.0.1",
    "serverless-step-functions": "^3.11.1",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "engines": {
    "npm": ">=8.19.2",
    "node": "~18.12.1"
  },
  "author": "Aligent Consulting",
  "license": "MIT"
}
