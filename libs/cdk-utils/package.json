{
  "name": "@libs/cdk-utils",
  "version": "0.0.1",
  "type": "module",
  "exports": {
    "./infra": {
      "types": "./src/infra.ts",
      "import": "./src/infra.ts"
    },
    "./runtime": {
      "types": "./src/runtime.ts",
      "import": "./src/runtime.ts"
    },
    "./package.json": "./package.json"
  },
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "nx": {
    "targets": {
      "build": {
        "executor": "nx:run-commands",
        "options": {
          "command": "tsc --build tsconfig.lib.json",
          "color": true,
          "cwd": "{projectRoot}"
        }
      }
    }
  }
}
